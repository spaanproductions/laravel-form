{
    "name": "spaanproductions/laravel-form",
    "description": "HTML and Form Builders for the Laravel Framework",
    "license": "MIT",
    "homepage": "https://spaanproductions.nl",
    "support": {
        "issues": "https://github.com/spaanproductions/laravel-form/issues",
        "source": "https://github.com/spaanproductions/laravel-form"
    },
    "authors": [
        {
            "name": "Adam Engebretson",
            "email": "adam@laravelcollective.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Sonny Spaan",
            "email": "info@spaanproductions.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/session": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^9.0|^10.0|^11.0|^12.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5.10|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "SpaanProductions\\LaravelForm\\": "src/"
        },
        "files": [
            "helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SpaanProductions\\LaravelForm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpaanProductions\\LaravelForm\\HtmlServiceProvider"
            ],
            "aliases": {
                "Form": "SpaanProductions\\LaravelForm\\FormFacade",
                "Html": "SpaanProductions\\LaravelForm\\HtmlFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
